{
  "name": "steemlookup",
  "version": "1.0.0",
  "description": "Frontend for Steemit filtering tool",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "devstart": "echo 'dont' && exit 2 && npm run watch & npm run nodemon",
    "postinstall": "npm run build",
    "build": "webpack --env.env=prod",
    "watch": "webpack --env.env=dev --watch",
    "nodemon": "nodemon --inspect server.js --ext js,json,jsx --ignore node_modules/ --ignore tests/ --ignore build/ --ignore 'app/client/**/*.js$'",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint app tests webpack client/* '*.js' --ext .js --ignore-pattern 'client/vendor/*.js'",
    "lint:js:fix": "npm run eslint -- --fix",
    "lint:css": "stylelint 'client/**/*.css'",
    "todo": "git grep -n '// TODO' -- './' ':!package.json' | sed 's/\\(.*:\\)\\([0-9]*\\):.*\\(\\/\\/ TODO.*\\)/\\1\\2 ==> \\3/' > TODO.md"
  },
  "author": "hcg.ninja",
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.0",
    "axios": "0.17.1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-regenerator-runtime": "6.5.0",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "cookie": "0.3.1",
    "cors": "2.8.4",
    "css-loader": "0.28.9",
    "datejs": "^1.0.0-rc3",
    "decode-html": "^2.0.0",
    "express": "4.16.2",
    "express-nunjucks": "2.2.3",
    "file-loader": "^1.1.6",
    "languagedetect": "^1.1.1",
    "lodash.clonedeep": "^4.5.0",
    "markdown": "^0.5.0",
    "node-minify-all": "^2.0.3",
    "nunjucks": "3.0.1",
    "pg": "^7.4.1",
    "postcss-loader": "2.0.10",
    "random-string": "^0.2.0",
    "steem": "0.7.1",
    "style-loader": "0.20.1",
    "tracer": "0.8.11",
    "vue": "2.5.13",
    "vue-loader": "13.7.0",
    "vue-router": "3.0.1",
    "vue-slider-component": "^2.5.3",
    "vue-template-compiler": "2.5.13"
  },
  "devDependencies": {
    "autoprefixer": "7.2.5",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "6.26.0",
    "extract-text-webpack-plugin": "3.0.2",
    "postcss-custom-media": "6.0.0",
    "postcss-custom-properties": "6.2.0",
    "postcss-import": "11.0.0",
    "postcss-nested": "3.0.0",
    "postcss-prefix-selector": "1.6.0",
    "webpack": "3.10.0",
    "webpack-notifier": "1.5.1"
  }
}
